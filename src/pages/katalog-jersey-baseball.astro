---
import BaseLayout from '../layouts/BaseLayout.astro';
---

<BaseLayout
  title="Katalog Jersey Baseball - Konveksi Satuan Surabaya Kustom Garment"
  description="Katalog Jersey Baseball - Konveksi Satuan Surabaya Kustom Garment"
>
  <div class="page-content-wrapper">
    <!-- Empty - page title is handled by the theme if needed -->
  </div>

  <div class="elementor elementor-990373">
    <section class="elementor-section elementor-section-boxed">
      <div class="elementor-container">
        <div class="elementor-column elementor-col-100">
          <div class="elementor-widget-wrap">
            <div class="elementor-element elementor-element-html">
              <div class="elementor-widget-container">
                <embed
                  src="/catalogs/20250820_Katalog-Jersey-Kustom_.pdf"
                  type="application/pdf"
                  width="100%"
                  height="800px"
                />
              </div>
            </div>
          </div>
        </div>
      </div>
    </section>
  </div>
</BaseLayout>

<style>
  /* Match Elementor default container styling */
  .elementor {
    width: 100%;
  }

  .elementor-section {
    position: relative;
  }

  .elementor-section-boxed {
    --elementor-section-max-width: 1140px;
  }

  .elementor-container {
    width: 100%;
    max-width: var(--elementor-section-max-width);
    margin: 0 auto;
  }

  .elementor-column {
    position: relative;
    min-height: 1px;
  }

  .elementor-col-100 {
    width: 100%;
  }

  .elementor-widget-wrap {
    position: relative;
    width: 100%;
  }

  .elementor-widget-container {
    position: relative;
    width: 100%;
  }

  embed {
    display: block;
    border: none;
  }
</style>
